{
  "name": "agentic-cost-calculator",
  "version": "1.0.0",
  "description": "Web-based tool for calculating AI LLM costs across development projects and ongoing usage",
  "main": "src/server/index.ts",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "NODE_ENV=development ts-node-dev --project tsconfig.server.json --transpile-only --respawn --require tsconfig-paths/register --files src/server/index.ts",
    "build": "tsc && vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx --fix"
  },
  "keywords": [
    "ai",
    "llm",
    "cost-calculator",
    "development",
    "agentic"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.1",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "cors": "2.8.5",
    "express": "^5.1.0",
    "jspdf": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "10.1.0",
    "recharts": "^2.15.3",
    "remark-gfm": "4.0.1",
    "remark-html": "16.0.1",
    "remark-parse": "11.0.0",
    "tailwindcss": "^3.4.17",
    "unified": "11.0.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "supertest": "7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "^6.3.2"
  }
}
